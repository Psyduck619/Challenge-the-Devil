page{
  height: 100vh;
}
.container{
  width: 100%;
  flex-direction: column;
  justify-content: start;
  align-items: center;
  overflow: auto;
  background-size: cover;
  background-repeat: no-repeat;
}
.radar{
  margin: 20rpx auto;
  width: 80%;
  border: rgb(119, 64, 12) solid 4px;
  border-radius: 25rpx;
  background-color: rgb(236, 236, 198);
  display: flex;
  flex-direction: column;
  justify-content: start;
  align-items: center;
}
.radar .tip{
  color: #BBBBBE;
  margin-bottom: 20rpx;
  font-size: 24rpx;
}
.s{
  width: 90%;
  height: 55%;
  margin: 20rpx auto;
  display: flex;
  flex-direction: column;
  justify-content: start;
  align-items: center;
  border: rgb(119, 64, 12) solid 4px;
  border-radius: 5rpx;
  background-color: rgb(236, 236, 198);
}
.block{
  margin: 20rpx 15rpx;
}
.block .title{
  display: flex;
  flex-direction: row;
  justify-content: start;
  align-items: center;
}
.block .title .text{
  font-size: 34rpx;
  font-weight: 700;
  line-height: 60rpx;
}
.block .text-wrap .text{
  font-size: 26rpx;
  line-height: 36rpx;
}
.block .icon{
  width: 33rpx;
  height: 33rpx;
  margin-right: 20rpx;
}
.block .plan{
  width: 33rpx;
  height: 20rpx;
}
.start{
  width: 400rpx;
  height: 120rpx;
  margin: 80rpx auto;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.button{
  width: 30%;
  border: rgb(119, 64, 12) solid 4px;
  border-radius: 20rpx;
  background-color: rgb(236, 236, 198);
  color: rgb(119, 64, 12);
  font-size: 42rpx;
  font-weight: 700;
  text-align: center;
  padding: 5rpx 0;
  margin-bottom: 30rpx;
}

/* 雷达图 */
.charts-module{
  position: relative;
  width: 100%;
  flex-direction: row;
  justify-content: center;
  margin-bottom: 27rpx;
}
.charts-radar{
  position: relative;
  width: 392rpx;
  height: 366rpx;
  color: #FFFFFF;
}

.charts-radar .radar-canvas-result{
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
  display: block;
}