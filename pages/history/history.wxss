page{
  height: 100vh;
}
.container{
  width: 100%;
  height: 100%;
  flex-direction: column;
  background-color: white;
  overflow: auto;
  background-size: cover;
  background-repeat: no-repeat;
}
.list-wrap{
  display: flex;
  flex-direction: column-reverse;
  justify-content: space-between;
  align-items: center;
}
.history-list, .null{
  height: 190rpx;
  width: 600rpx;
  margin: 30rpx auto;
  text-align: center;
  border: rgb(119, 64, 12) 3px solid;
  border-radius: 20rpx;
  background-color: rgb(236, 236, 198);
  box-shadow: 0px 2px 2px 2px rgb(197, 154, 89);
  -webkit-box-shadow: 0px 2px 2px 2px rgb(197, 154, 89);
  padding: 20rpx 20rpx 20rpx 20rpx;
}
.null{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  font-weight: 700;
}
.history-list .game{
  margin-bottom: 18rpx;
  flex-direction: row;
  justify-content: space-around;
}
.history-list .game1{
  color: #110bc4;
}
.history-list .game2{
  color: #505052;
}
.history-list .game3{
  color: #11A014;
}
.history-list .game4{
  color: #FF1010;
}
.history-list .bottom{
  flex-direction: row;
  justify-content: space-around;
  color: #BBBBBE;
  font-size: 24rpx;
  /* position: relative;
  left: -75rpx; */
}